elasticsearch.hosts: "https://elastic:9200"
server.host: "0.0.0.0"
server.port: 5601
elasticsearch.requestTimeout: 180000
logging:
  appenders:
    file:
      type: file
      fileName: /var/log/kibana/kibana.log
      layout:
        type: json
  root:
    appenders:
      - default
      - file
pid.file: /run/kibana/kibana.pid